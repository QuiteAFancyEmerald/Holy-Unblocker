{
  "name": "gatling",
  "version": "1.2.0",
  "description": "A simple node.js script that turns a single-threaded server into a multi-threaded server with automatic restarting.",
  "main": "gatling.js",
  "scripts": {
    "test": "npm run jshint && npm run spec",
    "spec": "mocha",
    "beautify": "js-beautify --replace ./*.js ./test/*.js",
    "jshint": "jshint ./ --exclude node_modules/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nfriedly/node-gatling"
  },
  "keywords": [
    "server",
    "fork",
    "cluster",
    "monitor",
    "forever"
  ],
  "author": "Nathan Friedly - http://nfriedly.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfriedly/node-gatling/issues"
  },
  "devDependencies": {
    "mocha": "~2.4.5",
    "request": "~2.69.0",
    "jshint": "~2.9.1",
    "js-beautify": "~1.6.2"
  },
  "dependencies": {
    "optimist": "~0.6.0"
  },
  "bin": {
    "gatling": "./gatling.js"
  }
}
